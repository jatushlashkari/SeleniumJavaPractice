<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Practice Suite" parallel="tests" thread-count="1">
    <listeners>
        <listener class-name="ExtentReport.Listeners.AnnotationTransformer"/>
        <listener class-name="ExtentReport.Listeners.TestListener"/>
    </listeners>
    <test name="ChromeTest">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="TestCases.LoginTest"/>
            <class name="TestCases.RegisterTest"/>
            <class name="TestCases.ShopTest"/>
        </classes>
    </test> <!-- Test -->
